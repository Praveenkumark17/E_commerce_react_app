{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "methods":["GET","POST","DELETE","PUT"],
      "headers":{
        "Access-Control-Allow-Origin":"*"
      }
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    }
  ]
}